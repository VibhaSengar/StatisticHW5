function createtheChart(canvas) {
    const chartData = {
        labels: [],
        datasets: [],
    };
 
    return new Chart(canvas.getContext("2d"), {
        type: "line",
        data: chartData,
        options: {
            scales: {
                y: {
                    beginAtZero: true,
                },
            },
            plugins: {
                legend: {
                    display: false,
                },
            },
        },
    });
}

